<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.josrom.finalfantasyx_listamonstruos" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/final_fantasy_x_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ListZones"
            android:label="@string/title_activity_list_zones"
            android:parentActivityName=".activity.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.josrom.finalfantasyx_listamonstruos.activity.MainActivity" />
        </activity>
        <activity
            android:name=".activity.ListMonsterZone"
            android:label="@string/title_activity_list_monster_zone"
            android:parentActivityName=".activity.ListZones" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.josrom.finalfantasyx_listamonstruos.activity.ListZones" />
        </activity>
    </application>

</manifest>
